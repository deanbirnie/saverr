  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deanbirnie/saverr/issues"
  },
  "homepage": "https://github.com/deanbirnie/saverr#readme",
  "nodemon": {
    "watch": [
      "back-end/*",
      "back-end/**/*",
      "front-end/*",
      "front-end/**/*"
    ],
    "ext": "js,jsx,ts,tsx,json"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6"
